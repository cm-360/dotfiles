            "enabled": true,
            "stringRules": [
                {
                    "find": ";disgust",
                    "replace": "https://<personal_domain>/emojis/disgusted-40.png",
                    "onlyIfIncludes": ""
                },
                {
                    "find": "",
                    "replace": "",
                    "onlyIfIncludes": ""
                }
            ],
            "regexRules": [
                {
                    "find": "",
                    "replace": "",
                    "onlyIfIncludes": ""
                }
            ]
